{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "dev": {
      "dependsOn": [
        "^build:vitepress-demo-block",
        "^build:vite-plugin-gen-temp",
        "^build:md-demo-plugins"
      ],
      "cache": false
    },
    "build:vite-plugin-gen-temp": {
      "outputs": ["dist/**"]
    },
    "build:md-demo-plugins": {
      "outputs": ["dist/**"]
    },
    "build:vitepress-demo-block": {
      "outputs": ["dist/**"]
    }
  }
}
